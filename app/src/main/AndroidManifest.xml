<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.calendar3">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Appointment"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CreateActivity"></activity>
        <activity android:name=".ViewActivity"></activity>
        <activity android:name=".DeleteActivity"></activity>
        <activity android:name=".EditActivity"></activity>
        <activity android:name=".SearchActivity"></activity>
        <activity android:name=".EditDataActivity"></activity>
        <activity android:name=".MoveActivity"></activity>
        <activity android:name=".SearchDataActivity"></activity>
        <activity android:name=".DeleteOptions"></activity>
    </application>

</manifest>